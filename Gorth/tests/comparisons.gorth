include "std/std.gorth"

func main
  : int
do
  // Equal
  1 2 = cast(int) println_num
  2 1 = cast(int) println_num
  1 1 = cast(int) println_num

  // Not Equal
  3 4 != cast(int) println_num
  4 3 != cast(int) println_num
  3 3 != cast(int) println_num

  // Less Than or Equal
  5 6 <= cast(int) println_num
  6 5 <= cast(int) println_num
  5 5 <= cast(int) println_num

  // Greater Than or Equal
  7 8 >= cast(int) println_num
  8 7 >= cast(int) println_num
  7 7 >= cast(int) println_num

  // Greater Than
  9 10 > cast(int) println_num
  10 9 > cast(int) println_num
  9 9  > cast(int) println_num

  // Less Than
  11 12 < cast(int) println_num
  12 11 < cast(int) println_num
  11 11 < cast(int) println_num

  0
end
