func main do
  // Equal
  1 2 = puti '\n' putc
  2 1 = puti '\n' putc
  1 1 = puti '\n' putc

  // Not Equal
  3 4 != puti '\n' putc
  4 3 != puti '\n' putc
  3 3 != puti '\n' putc

  // Less Than or Equal
  5 6 <= puti '\n' putc
  6 5 <= puti '\n' putc
  5 5 <= puti '\n' putc

  // Greater Than or Equal
  7 8 >= puti '\n' putc
  8 7 >= puti '\n' putc
  7 7 >= puti '\n' putc

  // Greater Than
  9 10 > puti '\n' putc
  10 9 > puti '\n' putc
  9 9  > puti '\n' putc

  // Less Than
  11 12 < puti '\n' putc
  12 11 < puti '\n' putc
  11 11 < puti '\n' putc
end
